<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />

<div class="event-schedule-area-two bg-color pad100">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="section-title text-center">
            </div>
            <!-- /.col end-->
        </div>
        <!-- row end-->
        <div class="row">
            <div class="col-lg-12">

                <div class="tab-content" id="myTabContent">
                    <div class="tab-pane fade active show" id="home" role="tabpanel">
                        <div class="table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th class="text-center" scope="col">Booking</th>
                                        <th scope="col">User</th>
                                        <th scope="col">Booking</th>
                                        <th scope="col">Venue</th>
                                        <th class="text-center" scope="col">Venue</th>
                                    </tr>
                                </thead>
                                <% @bookings.each do |booking| %>
                                  <tbody>
                                      <tr class="inner-box">
                                          <th scope="row">
                                              <div class="event-date">
                                                  <span><%= booking.id %></span>
                                              </div>
                                          </th>
                                          <td>
                                              <div class="event-img">
                                                  <img src="https://bootdey.com/img/Content/avatar/avatar1.png" alt="" />
                                              </div>
                                          </td>
                                          <td>
                                              <div class="event-wrap">
                                                  <h2><%= booking.user.first_name %> <%= booking.user.last_name %></h2>
                                                  <div class="meta">
                                                      <div class="organizers">
                                                          <h2>Address: <%= booking.user.street %> <%= booking.user.zip_code %> </h2>
                                                      </div>
                                                      <div class="categories">

                                                      </div>
                                                      <div class="time">
                                                          <span><%= booking.user.city %> </span>
                                                      </div>
                                                  </div>
                                              </div>
                                          </td>
                                          <td>
                                              <div class="r-no">
                                                  <span>Room B3</span>
                                              </div>
                                          </td>
                                          <td>
                                              <div class="primary-btn">
                                                  <a class="btn btn-primary" href="#">Read More</a>
                                              </div>
                                          </td>
                                      </tr>
                                <% end %>
            </div>
            <!-- /col end-->
        </div>
        <!-- /row end-->
    </div>
</div>
